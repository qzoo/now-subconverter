{
  "version": 2,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "sub-web/**",
      "use": "@vercel/static"
    },
    {
      "src": "subconverter/subconverter",
      "use": "@vercel/static-files"
    }
  ],
  "routes": [
    {
      "src": "/(js|css|fonts)/(.*)",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      },
      "dest": "sub-web/$1/$2"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "sub-web/index.html"
    },
    {
      "src": "/favicon\\.ico",
      "dest": "sub-web/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/subconverter/subconverter"
    }
  ]
}
